user nginx;
worker_processes auto;
error_log /var/log/nginx/error.log;
pid /run/nginx.pid;

events {
        worker_connections 1024;
}

http {
    upstream webapp {
      # for hostname, ip in hostname_ip.iteritems() 
      #   server ;
      # endfor 
    }

    server {
        listen 80;

        location / {
            proxy_pass http://webapp;
        }
    }   
}
