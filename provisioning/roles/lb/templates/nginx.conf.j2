user nginx;
worker_processes auto;
error_log /var/log/nginx/error.log;
pid /run/nginx.pid;

events {
        worker_connections 1024;
}

http {
    upstream webapp {
    {% for host in ip%}
      {% if host.ip != "192.168.2.10" %}
      server  {{ host.ip }};
      {% endif %}
    {% endfor %}
    }

    server {
        listen 80;

        location / {
            proxy_pass http://webapp;
        }
    }   
}
